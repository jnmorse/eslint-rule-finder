{
  "name": "eslint-rule-finder",
  "version": "1.3.2",
  "description": "Help Maintaining a Custom Eslint Config",
  "keywords": [
    "cli",
    "eslint",
    "eslintconfig"
  ],
  "homepage": "https://github.com/jnmorse/eslint-rule-finder#readme",
  "bugs": {
    "url": "https://github.com/jnmorse/eslint-rule-finder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnmorse/eslint-rule-finder.git"
  },
  "license": "MIT",
  "author": "Joseph Morse <tamed.lionheart@gmail.com>",
  "main": "index.js",
  "bin": {
    "eslint-rule-finder": "bin/find-rules"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint .",
    "prebuild": "rm -r dist",
    "build": "tsc",
    "postbuild": "chmode +x dist/bin/*.js",
    "prerelease": "npm run --silent build && npm run --silent test",
    "release": "standard-version --sign",
    "test": "mocha"
  },
  "dependencies": {
    "commander": "^2.20.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/lodash": "^4.14.135",
    "@types/node": "^12.0.10",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-config-jnmorse": "^2.5.0",
    "eslint-plugin-react": "^7.14.2",
    "mocha": "^6.1.4",
    "react": "^16.8.6",
    "standard-version": "^6.0.1"
  },
  "peerDependencies": {
    "eslint": ">= 6.0.1"
  }
}
