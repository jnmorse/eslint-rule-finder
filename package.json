  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.10.0",
    "eslint-config-jnmorse": "^1.2.1",
    "mocha": "^4.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jnmorse/eslint-rule-finder.git"
  },
  "bugs": {
